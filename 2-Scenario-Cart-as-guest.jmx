            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper;
import groovy.json.JsonOutput;
import java.util.Random;

//log.info for logging on jmeter console
//log.info (&quot;I am a test info log&quot;);
data = vars.get(&quot;digitalProductsFromCreator&quot;);
cartMinQty=vars.get(&quot;cartMinQty&quot;)toInteger();

//Get JSON response and transforms it to valid data
JsonSlurper slurper = new JsonSlurper();
ArrayList parsedData = slurper.parseText(data);
//log.info (&quot;Products:&quot;);
//log.info(parsedData.toString());

//Get quantity Of products
def productsQty = parsedData.size();
//log.info (&quot;Products qty:&quot;);
//log.info(productsQty.toString());
//Define the quantity in product of cart between min and max cart quantities
def productsCartQty = getRandomNumberInRange(cartMinQty, productsQty + 1);
//log.info (&quot;Random qty:&quot;);
//log.info(productsCartQty.toString());
def cart = [products: []];
//Don&apos;t repeat products
def indexesUsed = [];


while (cart.products.size() &lt; productsCartQty) {
	def index = getRandomNumberInRange(0, productsQty);
	//log.info (&quot;Random index:&quot;);
	//log.info(index.toString());
	if (!indexesUsed.contains(index)){
		def product = parsedData[index];
		cart.products.add([
			&apos;productId&apos;: product.product.productId.toString(),
			&apos;variantId&apos;: product.product.variantId.toString(),
			&apos;livestreamId&apos;: product._id,
			&apos;quantity&apos;: 1
		]);
	}
	indexesUsed.add(index);
}

/*for(product in parsedData) { 
	// Reads product object value
	//log.info(product.toString());
	cart.products.add([
		&apos;productId&apos;: product.product.productId,
		&apos;variantId&apos;: product.product.variantId,
		&apos;livestreamId&apos;: product._id
	]);
}

log.info(&quot;Cart:&quot;);
log.info(cart.toString());
log.info(&quot;cart qty:&quot;);
log.info(cart.products.size().toString());*/

//Put cart into jmeter variables
vars.put(&quot;cart&quot;, JsonOutput.toJson(cart));

//Definition of a helper function. Upper is not included
def getRandomNumberInRange(int lower, int upper){
	Math.abs( new Random().nextInt() % (upper - lower) ) + lower;
}


</stringProp>
